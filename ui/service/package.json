{
  "name": "node-js-service",
  "version": "1.0.0",
  "description": "service for working with the beam api",
  "main": "server.ts",
  "scripts": {
    "dev": "nodemon -L ./src/server.ts",
    "build": "tsc --project .",
    "start": "ts-node ./src/server.ts",
    "lint": "eslint --ext .js,.ts --fix src/",
    "test": "npm run test",
    "debug": "node --inspect=0.0.0.0:9229 --require ts-node/register src/server.ts",
    "watch-bundle:server": "webpack --watch",
    "watch-compile:server": "tsc --watch",
    "bundle:server": "webpack"
  },
  "author": "Danique",
  "license": "ISC",
  "dependencies": {
    "@types/nodegit": "^0.27.9",
    "commander": "^9.0.0",
    "cors": "^2.8.5",
    "dotenv": "^11.0.0",
    "express": "^4.17.2",
    "nodegit": "^0.27.0",
    "request": "^2.88.2",
    "winston": "^3.4.0"
  },
  "devDependencies": {
    "@types/commander": "^2.12.2",
    "@types/cors": "^2.8.12",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.13",
    "@types/request": "^2.48.8",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "eslint": "^8.6.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-plugin-node": "^11.1.0",
    "jest": "^27.4.7",
    "nodemon": "^2.0.15",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4",
    "webpack": "^5.67.0",
    "webpack-cli": "^4.9.2",
    "webpack-node-externals": "^3.0.0"
  }
}
